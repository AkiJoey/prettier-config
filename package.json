{
  "name": "@akijoey/prettier-config",
  "version": "0.0.3",
  "description": "Personal prettier config.",
  "main": "index.js",
  "files": [
    "index.js",
    ".prettierrc.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akijoey/prettier-config.git"
  },
  "keywords": [
    "prettier"
  ],
  "author": "AkiJoey <akijoey@akijoey.com> (https://akijoey.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akijoey/prettier-config/issues"
  },
  "homepage": "https://github.com/akijoey/prettier-config#readme",
  "devDependencies": {
    "@akijoey/eslint-config": "latest",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0"
  },
  "peerDependencies": {
    "prettier": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "@akijoey"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,json,yml,md}": "prettier --write"
  }
}
